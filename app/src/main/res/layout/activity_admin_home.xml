<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminHomeActivity">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="400px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo2" />

    <Button
        android:id="@+id/viewStudBtn"
        android:layout_width="162dp"
        android:layout_height="59dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="77dp"
        android:backgroundTint="#17651A"
        android:text="Student List"
        app:cornerRadius="100px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/viewResBtn"
        android:layout_width="162dp"
        android:layout_height="59dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="35dp"
        android:backgroundTint="#17651A"
        android:text="Restaurant List"
        app:cornerRadius="100px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewStudBtn" />

    <Button
        android:id="@+id/adminLogoutBtn"
        android:layout_width="109dp"
        android:layout_height="49dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="64dp"
        android:backgroundTint="#E91E63"
        android:text="Log out"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewResBtn" />
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>